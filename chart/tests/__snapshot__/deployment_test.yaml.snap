operator match the snapshot:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      name: RELEASE-NAME
      namespace: NAMESPACE
    spec:
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          service: RELEASE-NAME
      template:
        metadata:
          annotations:
            traffic.sidecar.istio.io/excludeInboundPorts: "8080"
          labels:
            control-plane: controller-manager
            service: RELEASE-NAME
        spec:
          containers:
            - args:
                - --leader-elect
                - --log-level=
                - --health-probe-bind-address=:8081
              image: :latest
              livenessProbe:
